import Foundation
import Utilities

func doperf() -> Float64
{
	var sum: Float64 = 0.0
	var j: Int64 = 0

	while j < 500
	{
		sum = 0
		var k: Int64 = 1

		while k <= 10000
		{
			sum += 1.0 / (k * k)
			k += 1
		}

		j += 1
	}

	return sum
}

func main(argc: Int32, argv: Int8**) -> Int64
{
	var sum: Float64 = doperf()

	printf("%lf\n", sum)
	return 0
}
