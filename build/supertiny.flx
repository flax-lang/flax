export supertiny

import "libc"

@entry fn main()
{
	for it in 1...5 step 2 => libc.printf("%d ", it)
	libc.printf("\n")

	for c in "HELLO" => libc.printf("%c", c)
	libc.printf("\n")

	for it in [ 1, 1, 2, 3, 5, 8, 13 ] => libc.printf("%d ", it)

	libc.printf("\b\b\n");
}




