var glob: any

	struct Large
	{
		var a: i64 = 10
		var b: i64 = 20
		var c: i64 = 30
		var d: i64 = 40
		var e: i64 = 50
	}

public func doAnyTest()
{
	func foo(a: any)
	{
		if(typeid(a) == typeid(Large))
		{
			printf("a = %d\n", (a as Large).a)
			glob = a
		}
		else
		{
			printf("not large\n")
		}
	}

	do {
		var k: any = 40 as any
		k = "foo"

		printf("k as string = %s\n", k as string)

		k = 301

		printf("k as int = %d\n", k as int)

		printf("typeid(k): %d\n", typeid(k))
	}

	do {
		var x: any = Large()
		// (x as Large).c = 301443

		// let v = x// as Large
		var k = x as Large
		k.c = 5413
		k.a = 631

		foo(x)
		foo(k)
		foo(401.3)

		func bar() -> any { 10 }

		printf("bar = %d, qux = %d\n", bar(), 30)
		printf("a = %d, b = %d, c = %d/%d, d = %d, e = %d\n", k.a, k.b, k.c, (x as Large).c, k.d, k.e)
	}
}






