import Foundation

public Æ’ doFizzBuzz(var i: Int)
{
	while i > 0
	{
		printf("%d: ", i)

		if i % 3 == 0
		{
			printf("Fizz")
		}
		if i % 5 == 0
		{
			printf("Buzz")
		}

		printf("\n")
		i -= 1
	}
}
