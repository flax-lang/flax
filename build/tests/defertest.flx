import Foundation


public func doDeferTest()
{
	bar()
}


func foo(x: int) -> int
{
	printf("calling foo %d\n", x)
	return 2 * x
}


func bar() -> int
{
	foo(10)
	defer foo(20)
	return foo(30)
}


