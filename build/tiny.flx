ffi func printf(fmt: i8*, ...) -> int

public func main() -> int
{
	do {
		var k = 40 as any
		k = "foo"

		printf("k as string = %s\n", k as string)

		k = 301 as int

		printf("k as int = %d\n", k as int)

		// printf("typeof(k): %d\n", typeof(k))
	}

	return 0
}
