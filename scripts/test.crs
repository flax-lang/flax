import Foundation

struct String
{
	var data: Int8 ptr
	var len: Int64

	init()
	{
		self.len = 64;
		self.data = malloc(64) as Int8 ptr
	}

	func length() -> Int64
	{
		return self.len
	}
}



func main()
{
	var s: String
	s.length()
}

ffi func printBool(x: Bool)
ffi func printInt64(x: Int64)
ffi func putchar(x: Int32) -> Int32
ffi func malloc(x: Int64) -> Int8 ptr
