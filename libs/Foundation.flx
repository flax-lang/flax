import LibCInterface
func min(a: Uint64, b: Uint64) -> Uint64
{
	if a < b
	{
		return a
	}

	return b
}

func max(a: Uint64, b: Uint64) -> Uint64
{
	if a > b
	{
		return a
	}

	return b
}




