import Core
import LibCInterface
import IntegerLimits

public func min(a: Uint64, b: Uint64) -> Uint64
{
	if a < b
	{
		return a
	}

	return b
}

public func max(a: Uint64, b: Uint64) -> Uint64
{
	if a > b
	{
		return a
	}

	return b
}

public func print(fmt: String, args: Any[...])
{
	printf("%s", fmt)
}

public func println()
{
	printf("\n");
}















