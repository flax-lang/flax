import String

public protocol StringConvertible
{
	func toString() -> String
}

public protocol Numeric
{

}

public protocol Equatable
{
	func isEqual(other: Self) -> bool
}


public operator == <T: Equatable> (a: T, b: T) -> bool
{
	return a.isEqual(b)
}



